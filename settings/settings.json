{
    "1": "====================ENVIRONMENT====================",

    "width": 10,
    "height": 10,
    "sight_len": 3,
    "num_obj_types": 2,
    "num_agents": 5,
    "initial_food_density": 1.0,
    "initial_food_regen_prob": 1.0,
    "adaptive_food_type": "food",
    "target_density": 0.07,
    "food_size_mean": 1.0,
    "food_size_stddev": 0.00,
    "aging_rate": 1e-6,
    "aging_type": "linear",
    "mating_cooldown_len": 1e12,
	"policy_score_mating_threshold": 0.2,
    "print_repr": false,
    "num_displayed_agents": 20,

    "2": "====================TRAINER====================",

    "time_steps": 2000,
    "reuse_state_dicts": true,
    "policy_score_frequency": 8,
    "ema_alpha": 0.999,
    "greedy_temperature": 1e-4,
    "mp": false,

    "3": "====================REWARD====================",

    "n_layers": 1,
    "hidden_dim": 1,
    "reward_weight_mean": 0.0,
    "reward_weight_stddev": 0.4,
    "reward_inputs": ["obs", "health"],
    "tabular": true,

    "4": "====================GENETICS====================",

    "mut_sigma": 0.3,
    "mut_p": 0.05,

    "5": "====================CONSTANTS====================",

    "STAY": 0,
    "LEFT": 1,
    "RIGHT": 2,
    "UP": 3,
    "DOWN": 4,
    "EAT": 0,
    "NO_EAT": 1,
    "MATE": 0,
    "NO_MATE": 1,
    "HEAVEN": [-1,-1],

    "6": "====================POLICY====================",

    "algo": "ppo",
    "lr": 3e-4,
    "min_lr": 5e-6,
    "eps": 1e-5,
    "alpha": 1e-5,
    "gamma": 0.99,
    "use_gae": false,
    "gae_lambda": 0.95,
    "entropy_coef": 0.01,
    "value_loss_coef": 1.0,
    "max_grad_norm": 0.5,
    "seed": 2,
    "cuda_deterministic": false,
    "num_processes": 1,
    "num_steps": 256,
    "ppo_epoch": 4,
    "num_mini_batch": 1,
    "clip_param": 0.3,
    "log_interval": 10,
    "save_interval": 2048,
    "env_name": "bees",
    "log_dir": "/tmp/gym/",
    "cuda": true,
    "use_proper_time_limits": false,
    "base_kwargs": {
        "recurrent": false,
        "hidden_size": 32,
        "num_layers": 4,
        "initial_num_channels": 32
    },
    "use_linear_lr_decay": false
}
