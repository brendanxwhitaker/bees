{
    "1": "====================ENVIRONMENT====================",

    "width": 10,
    "height": 10,
    "sight_len": 6,
    "num_obj_types": 2,
    "num_agents": 5,
    "food_density": 0.2,
    "food_size_mean": 0.29,
    "food_size_stddev": 0.247,
    "plant_foods_mean": 0.01,
    "plant_foods_stddev": 0.2,
    "food_plant_retries": 3,
